{
    "mcpServers": {
        "fetch": {
            "type": "stdio",
            "command": "uvx",
            "args": [
            "-q",
            "mcp-server-fetch"
            ]
        },
        "filesystem": {
            "type": "stdio",
            "command": "npx",
            "args": [
            "-y",
            "@modelcontextprotocol/server-filesystem",
            "."
            ]
        },
        "everything": {
            "type": "sse",
            "url": "http://localhost:6969/sse"
        }
    }
}
